<h3>ユーザー編集</h3>

 <%= form_with model: @user do |f| %>
  <table>
    <tr>
      <th><%= f.label :name ,"名前" %></th>
      <td><%= f.text_field :name %></td>
    </tr>
    <tr>
      <th><%= f.label :name, "会社名" %></th>
      <td><%= f.text_field :company %></td>
    </tr>
  </table>

  <div>
    <ul>
      <li><%= f.submit "編集を完了する" %></li>
    </ul>
  </div>
 <% end %>